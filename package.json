{
  "name": "bugajski.io",
  "version": "0.0.0",
  "private": false,
  "scripts": {
    "dev": "cross-env NODE_OPTIONS=--no-deprecation next dev",
    "i:dev": "infisical run pnpm dev",
    "dev:turbo": "cross-env NODE_OPTIONS=--no-deprecation next dev --turbo",
    "i:dev:turbo": "infisical run pnpm dev:turbo",
    "devsafe": "rm -rf .next && cross-env NODE_OPTIONS=--no-deprecation next dev",
    "i:devsafe": "infisical run pnpm devsafe",
    "build": "cross-env NODE_OPTIONS=--no-deprecation next build",
    "i:build": "infisical run pnpm build",
    "payload": "cross-env NODE_OPTIONS=--no-deprecation payload",
    "i:payload": "infisical run pnpm payload",
    "start": "cross-env NODE_OPTIONS=--no-deprecation next start",
    "i:start": "infisical run pnpm start",
    "generate:types": "infisical run pnpm payload generate:types -- --use-tsx",
    "generate:importmap": "infisical run pnpm payload generate:importmap -- --use-tsx",
    "lint": "cross-env NODE_OPTIONS=--no-deprecation next lint",
    "lint:all": "pnpm prettier && pnpm eslint && pnpm stylelint && pnpm lint",
    "eslint": "eslint --ext .js,.jsx,.ts,.tsx .",
    "eslint:fix": "eslint --fix --ext .js,.jsx,.ts,.tsx .",
    "prettier": "prettier --check --ignore-unknown .",
    "prettier:fix": "prettier --write --ignore-unknown .",
    "stylelint": "stylelint '**/*.css' --ignore-path .gitignore",
    "stylelint:fix": "stylelint '**/*.css' --fix --ignore-path .gitignore"
  },
  "packageManager": "pnpm@9.12.2",
  "dependencies": {
    "@payloadcms/db-mongodb": "3.0.0-beta.116",
    "@payloadcms/email-resend": "3.0.0-beta.116",
    "@payloadcms/next": "3.0.0-beta.116",
    "@payloadcms/richtext-lexical": "3.0.0-beta.116",
    "@payloadcms/ui": "3.0.0-beta.116",
    "@t3-oss/env-nextjs": "^0.11.1",
    "@vercel/speed-insights": "^1.0.12",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.1",
    "cross-env": "^7.0.3",
    "geist": "^1.3.1",
    "luxon": "^3.5.0",
    "next": "15.0.0-canary.173",
    "payload": "3.0.0-beta.116",
    "react": "19.0.0-rc-3edc000d-20240926",
    "react-dom": "19.0.0-rc-3edc000d-20240926",
    "tailwind-merge": "^2.5.4",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@types/luxon": "^3.4.2",
    "@types/node": "20.14.10",
    "@types/react": "npm:types-react@19.0.0-rc.1",
    "@types/react-dom": "npm:types-react-dom@19.0.0-rc.1",
    "@typescript-eslint/eslint-plugin": "^8.10.0",
    "@typescript-eslint/parser": "^8.10.0",
    "eslint": "^8.57.1",
    "eslint-config-next": "15.0.0-canary.173",
    "eslint-config-prettier": "^9.1.0",
    "eslint-import-resolver-typescript": "^3.6.3",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-jsx-a11y": "^6.10.0",
    "eslint-plugin-prettier": "^5.2.1",
    "postcss": "^8.4.47",
    "prettier": "^3.3.3",
    "prettier-plugin-tailwindcss": "^0.6.8",
    "stylelint": "^16.10.0",
    "stylelint-config-clean-order": "^6.1.0",
    "stylelint-config-standard": "^36.0.1",
    "tailwindcss": "^3.4.14",
    "tsx": "^4.19.1",
    "typescript": "5.6.3"
  },
  "pnpm": {
    "overrides": {
      "@types/react": "npm:types-react@19.0.0-rc.1",
      "@types/react-dom": "npm:types-react-dom@19.0.0-rc.1"
    }
  },
  "overrides": {
    "@types/react": "npm:types-react@19.0.0-rc.1",
    "@types/react-dom": "npm:types-react-dom@19.0.0-rc.1"
  }
